{
  "name": "@sailfishdex/v3-sdk",
  "version": "1.1.10",
  "description": "SDK for SailFish DEX v3",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dist/ui/assets",
    "dist/ui/fonts/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "build-dev": "rollup -c && npm link && cd examples/nextjs-bridge-example && npm link @sailfishdex/v3-sdk",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sailfish",
    "dex",
    "sdk",
    "ethereum",
    "educhain"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SailFish-Finance/v3-sdk"
  },
  "author": "SailFish Team",
  "license": "MIT",
  "dependencies": {
    "@uniswap/sdk-core": "^3.2.2",
    "@uniswap/v3-sdk": "^3.9.0",
    "axios": "^1.8.3",
    "bignumber.js": "^9.1.1",
    "ethers": "^6.7.1",
    "graphql": "^16.8.0",
    "graphql-request": "^6.1.0",
    "jsbi": "^4.3.0",
    "styled-jsx": "^5.1.6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-url": "^8.0.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "rollup": "^4.35.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
